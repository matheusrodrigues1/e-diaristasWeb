<div class='content-header'>
  <div class='cointainer-fluid'>
    <h1>Lista de Usuarios</h1>
  </div>
</div>
<div class='content'>
  <div class='container-fliud'>
    <table class='table'>
      <thead>
        <tr>
          <th scope='col'>Id
          </th>
          <th scope='col'>Nome
          </th>
          <th scope='col'>Email
          </th>
          <th scope='col'>Ações
          </th>
        </tr>
      </thead>
      <tbody>
        {{#each usuarios}}
          <tr>
            <th>{{this.id}}</th>
            <td>{{this.nome}}</td>
            <td>{{this.email}}</td>
            <td><a href='#' class='btn btn-primary'>Atualizar</a>
              <a href='#' class='btn btn-danger'>Excluir</a>
            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
    <div class='float-left'>
      <a href='#' class='btn btn-success'>Novo Usuario</a>
    </div>
  </div>
</div>