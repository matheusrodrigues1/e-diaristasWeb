<div class='content-header'>
  <div class='cointainer-fluid'>
    <h1>Lista de Serviços</h1>
  </div>
</div>
<div class='content'>
  <div class='container-fliud'>
    <table class='table'>
      <thead>
        <tr>
          <th scope='col'>Id
          </th>
          <th scope='col'>Nome
          </th>
          <th scope='col'>Ações
          </th>
        </tr>
      </thead>
      <tbody>
        {{#each servicos}}
          <tr>
            <th>{{this.id}}</th>
            <td>{{this.nome}}</td>
            <th><a
                href='/admin/servicos/{{this.id}}/edit'
                class='btn btn-primary'
              >Atualizar</a></th>
          </tr>
        {{/each}}
      </tbody>
    </table>
    <div class='float-left'>
      <a href='/admin/servicos/create' class='btn btn-success'>Novo Serviço</a>
    </div>
  </div>
</div>